    "metrics_net_if": {
      "type": "metric",
      "command": "metrics-interface.rb",
      "interval": 20,
      "handlers": [ "metrics" ],
      "subscribers": [ "metrics_os" ]
    },
    "metrics_net_sockstat": {
      "type": "metric",
      "command": "metrics-sockstat.rb",
      "interval": 20,
      "handlers": [ "metrics" ],
      "subscribers": [ "metrics_os" ]
    }
  }
}
