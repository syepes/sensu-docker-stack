{
  "checks": {
    "metrics_load": {
      "type": "metric",
      "command": "metrics-load.rb",
      "interval": 20,
      "handlers": [ "metrics" ],
      "subscribers": [ "metrics_os" ]
    },
    "metrics_cpu": {
      "type": "metric",
      "command": "metrics-cpu.rb",
      "interval": 20,
      "handlers": [ "metrics" ],
      "subscribers": [ "metrics_os" ]
    },
    "metrics_memory": {
      "type": "metric",
      "command": "metrics-memory.rb",
      "interval": 20,
      "handlers": [ "metrics" ],
      "subscribers": [ "metrics_os" ]
    },
    "metrics_memory_pct": {
      "type": "metric",
      "command": "metrics-memory-percent.rb",
      "interval": 20,
      "handlers": [ "metrics" ],
      "subscribers": [ "metrics_os" ]
    },
    "metrics_disk": {
      "type": "metric",
      "command": "metrics-disk.rb",
      "interval": 20,
      "handlers": [ "metrics" ],
      "subscribers": [ "metrics_os" ]
    },
    "metrics_disk_usage": {
      "type": "metric",
      "command": "metrics-disk-usage.rb",
